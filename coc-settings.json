{
    "coc.preferences.extensionUpdateCheck": "daily",
    "coc.preferences.maxFileSize": "25MB",
    "coc.preferences.formatOnSaveFiletypes": [
        "json",
        "xml"
    ],
    "workspace.ignoredFolders": [
        "$HOME",
        "./git/",
        "./svn/"
    ],
    "workspace.ignoredFiletypes": [
        "*.a",
        "*.o",
        "*.so",
        "*.d"
    ],
    "workspace.openResourceCommand": "tab drop",
    "git.addGBlameToBufferVar": true,
    "git.addGBlameToVirtualText": true,
    "git.showCommitInFloating": false,
    "git.changeRemovedSign.text": "",
    "markdownlint.onOpen": true,
    "markdownlint.onChange": false,
    "markdownlint.onSave": true,
    "diagnostic.refreshOnInsertMode": false,
    "diagnostic.showUnused": false,
    "diagnostic.warningSign": "",
    "diagnostic.errorSign": "",
    "suggest.noselect": true,
    "suggest.enablePreselect": false,
    //"clangd.path": "~/.config/coc/extensions/coc-clangd-data/install/13.0.0/clangd_13.0.0/bin/clangd",
    "clangd.disableSnippetCompletion": false,
    "languageserver": {
        // "golang": {
        //     "command": "gopls",
        //     "rootPatterns": [
        //         "go.mod",
        //         ".vim/",
        //         ".git/",
        //         ".hg/"
        //     ],
        //     "filetypes": [
        //         "go"
        //     ],
        //     "initializationOptions": {
        //         "usePlaceholders": true
        //     }
        // }
    },
    "Lua.runtime.version": "Lua 5.2",
    "Lua.diagnostics.globals": [
        "Game",
        "g_BingoMgr",
        "pb",
        "NULL_UUID",
        "EResourceCostReason",
        "currentTime",
        "DAY_SECONDS",
        "g_handleMgr",
        "EResourceAddReason"
    ],
    "list.source.mru.excludePatterns": [
        "**/.git/*",
        "/tmp/*",
        "**/.svn/*"
    ],
    "list.source.grep.excludePatterns": [
        "**/.git/*",
        "/tmp/*",
        "**/.svn/*",
        ".git/*",
        ".svn/*"
    ],
    "list.source.files.defaultOptions": [
        "--number-select"
    ],
    "Lua.workspace.checkThirdParty": false,
    "Lua.telemetry.enable": true,
    "tsserver.log": "verbose",
    "sh.enable": true,
    "git.enableGutters": true
}
